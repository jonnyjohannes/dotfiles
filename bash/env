export EDITOR=vi

export PATH=/usr/local/bin:/usr/local/heroku/bin:/opt/local/bin:/opt/local/sbin:$PATH
export PATH=~/ucalgary/iqst/qmmm/tao/bin:$PATH ### TAO executables path ###
export PATH=/usr/local/share/python:$PATH ### Homebrew Python ###

[[ -s "$HOME/.rvm/scripts/rvm" ]] && source "$HOME/.rvm/scripts/rvm" # Load RVM into a shell session *as a function*
[[ -s $HOME/.virtualenvs/virtualenvwrapper.sh ]] && source $HOME/.virtualenvs/virtualenvwrapper.sh # Load Virtualenv
source $HOME/.dotfiles/git/git-completion.bash # Extra git autocomplete scripts

