# set vim as default
export EDITOR=vi
export VISUAL=$EDITOR

# Homebrew and custom compiled PATHs
export PATH=/usr/local/bin:/usr/local/sbin:$PATH
export PATH=$HOME/src/bin:$HOME/src/cuby:$PATH

# rbenv and virtualenv
eval "$(rbenv init -)"
[[ -s "$HOME/.venvburrito/startup.sh" ]] && source "$HOME/.venvburrito/startup.sh"

# autocomplete scripts
source $HOME/.dotfiles/git/git-completion.bash
source $HOME/.dotfiles/tmux/tmuxinator.bash

